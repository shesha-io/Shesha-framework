{
  "name": "shesha-reactjs-starter",
  "version": "0.1.0",
  "private": false,
  "postinstall": "next",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "NODE_ENV=production node --max-old-space-size=8192 server.js",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint **/*.{js,jsx,ts,tsx}",
    "prettier": "prettier --write **/*.{js,jsx,ts,tsx}",
    "test": "jest -c test/jest.config.js",
    "generate-fetcher": "restful-react import --config restful-react.config.js",
    "postgenerate-fetcher": "prettier ./src/api/*.tsx --write"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@microsoft/applicationinsights-react-js": "^3.1.2",
    "@microsoft/applicationinsights-web": "^2.6.2",
    "@microsoft/signalr": "^5.0.6",
    "@next/bundle-analyzer": "^10.0.5",
    "@shesha/reactjs": "^4.4.1",
    "@use-it/interval": "^1.0.0",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.24.0",
    "axios": "^0.21.1",
    "camelcase": "^6.2.0",
    "config": "^3.3.6",
    "file-saver": "^2.0.5",
    "filesize": "^6.3.0",
    "history": "^5.3.0",
    "htmlescape": "^1.1.1",
    "jodit-react": "^1.0.87",
    "jodit-react-ts": "^0.8.0",
    "js-encoding-utils": "^0.6.2",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "nanoid": "^3.1.23",
    "next": "^12.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-plugin-antd-less": "^1.4.3",
    "nprogress": "^0.2.0",
    "number-to-words": "^1.2.4",
    "point-in-polygon": "^1.1.0",
    "postcss": "^8.3.5",
    "prettier": "^2.2.1",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.0.2",
    "react-error-boundary": "^1.2.5",
    "react-json-view": "^1.21.3",
    "react-lazylog": "^4.5.3",
    "react-places-autocomplete": "^7.3.0",
    "react-sortablejs": "^6.0.0",
    "react-spinners": "^0.11.0",
    "react-table": "^7.7.0",
    "react-use": "^15.3.8",
    "redux-undo": "^1.0.1",
    "restful-react": "^15.9.2",
    "sortablejs": "^1.14.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-lazylog": "^4.5.0",
    "@types/redux-actions": "^2.3.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nested-property": "^4.0.0",
    "nock": "^13.0.6",
    "redux-actions": "^2.6.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
